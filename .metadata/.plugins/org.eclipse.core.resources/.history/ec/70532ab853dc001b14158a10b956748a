public class Demo {

	public static void main(String[] args) {
		Scanner cs = new Scanner(System.in);
		System.out.println("Number = ");
	
			if(cs.hasNextInt()) { 
				System.out.println("Good");	
			}else {
				System.out.println("please Enter a number");
				}



	}

}
